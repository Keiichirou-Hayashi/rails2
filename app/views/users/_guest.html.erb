<header class="header">
      <div class="header-inner">
        <div class="header-left-nav">
            <a href="/">
              <img alt="ロゴの画像" src="/assets/logo.png">
            </a>
            <%= search_form_for(@q, url: search_path) do |f| %>
              <div class="header-sharch d-lg-inline-block d-none">
                <%= f.search_field :rooms_address_cont, class: "form-control", placeholder: "エリア" %>
                <i class="fas fa-map-marker-alt">
                </i>
              </div>
              <div class="header-sharch d-lg-inline-block d-none">
                <%= f.search_field :rooms_introduction_cont, class: "form-control", placeholder: "キーワード" %>
                <i class="fas fa-search">
                </i>
              </div>
              <%= f.submit "", class: "d-none", data:{disable_with: ""} %>
            <% end %>
        </div>
        <div class="header-right-nav">
          <div class="nav-item">
            <a class="btn btn-primary my-2 mx-3" href="/users/sign_in">ログイン</a>
            <a class="btn btn-secondary" href="/users/sign_up">登録する</a>
          </div>
        </div>
      </div>
    </header>
    